	<queryString language="SQL">
		<![CDATA[SELECT p.prod_id,
	p.prod_nome,
	c.cat_nome,
	p.prod_descr,
	p.prod_preco
	
FROM produto p
	JOIN categoria c ON 
	 p.cat_id = c.cat_id 
ORDER BY p.prod_nome ]]>
	</queryString>
